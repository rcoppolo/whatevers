<!DOCTYPE html>
<html>
<head>
  <title>Whatevers</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<%= render 'shared/strip' %>
	<div id="container">

		<% flash.each do |key, msg| %>
			<%= content_tag :div, msg, :class => ["something flash #{key}"] %>
		<% end %>

		<%= yield %>

	</div>

</body>
</html>
